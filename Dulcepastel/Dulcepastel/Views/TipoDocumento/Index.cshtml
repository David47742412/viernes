@model IEnumerable<Dulcepastel.Models.utility.structView.GenericView>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Tipo de Documento</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Código
            </th>
            <th>
                Descripción
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @item.Value1
            </td>
            <td>
                @item.Value2
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Value1">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Value1">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Value1">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
